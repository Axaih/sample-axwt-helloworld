@import url("reset.gss");
@import url("type.gss");

html,
body {
  min-height: 100%;
  top: 0px;
  margin: 0;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 20px;
  color: #333333;
  background-color: #ffffff;
}
#wrap {
  top: 0px;
  min-height: 600px;
}
#main {
  overflow: visible;
  position: absolute;
  size: auto;
  width: 100%;
  min-height: 100%;
  height: 100%;
}
.bottomRightCorner {
  vertical-align: bottom;
  text-align: right;
  width: 100%;
}
.rightLinks {
  width: 100%;
  float: right;
}
