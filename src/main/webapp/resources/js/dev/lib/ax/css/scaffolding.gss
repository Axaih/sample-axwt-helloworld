/*@provide ax#gss#scaffolding; */


.empty {
float: left;
min-height: 1em;
width: 0px;
}

.row {
  margin-left: 0px;
  *zoom: 1;
}

.row:before,
.row:after {
  display: table;
  line-height: 0;
  content: "";
}

.row:after {
  clear: both;
}

[class*="span"] {
  float: left;
  min-height: 1px;
  margin-left: 0px;
}

.container {
  margin-right: 0;
  margin-left: auto;
   width: 960px; 
}

.ax-scaffolding-leftAligned {
margin-left: 0px;
}

.container:before,
.container:after {
  display: table;
  line-height: 0;
  content: "";
}

.container:after {
  clear: both;
}

 

